@model dotnet.Model.Tag
@using dotnet.Services.Extensions
@{
    long ID = RandomGenerator.GenerateID();
    int br = RandomGenerator.Generate_0_255();
    int bg = RandomGenerator.Generate_0_255();
    int bb = RandomGenerator.Generate_0_255();
    int fr = (br + 128)%256;
    int fg = (bg + 128)%256;
    int fb = (bb + 128)%256;
}

<div class="tag" id="@ID" draggable="true" style="background-color: rgb(@br,@bg,@bb);">
    <div class="ID">@Model.ID</div>
    <div class="_Tag" style="color: rgb(@fr,@fg,@fb);">@Model._Tag</div>
</div>



